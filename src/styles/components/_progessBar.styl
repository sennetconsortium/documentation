.c-progressBar
  width 150px
  height 20px
  border-radius 5px
  margin 20px 10px
  border 1px solid rgb(189, 113, 113)
  overflow hidden
  position relative
  span
    height 100%
    display block
    width 0
    color rgb(255, 251, 251)
    line-height 30px
    position absolute
    text-align end
    padding-right 5px